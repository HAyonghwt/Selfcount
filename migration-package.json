{
  "name": "firebase-migration-tool",
  "version": "1.0.0",
  "description": "Firebase Realtime Database 마이그레이션 도구",
  "main": "firebase-migration.js",
  "scripts": {
    "backup": "node firebase-migration.js --mode=backup",
    "migrate": "node firebase-migration.js --mode=migrate", 
