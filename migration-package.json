    "full-migration": "node firebase-migration.js --mode=full",
    "install-deps": "npm install firebase-admin"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0"
  },
  "keywords": [
    "firebase",
    "migration",
    "database",
    "backup"
  ],
